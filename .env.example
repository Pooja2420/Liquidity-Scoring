# Copy to .env and fill in values

# Optional: FRED API key for real macro factor data
# Register free at https://fred.stlouisfed.org/docs/api/api_key.html
FRED_API_KEY=

# MLflow tracking server (default: local mlruns/ directory)
MLFLOW_TRACKING_URI=mlruns

# API configuration
API_HOST=0.0.0.0
API_PORT=8000
API_WORKERS=4

# Model settings
N_CUSIPS=200          # Number of synthetic CUSIPs
RANDOM_SEED=42
